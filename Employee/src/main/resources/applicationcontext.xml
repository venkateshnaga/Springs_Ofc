<?xml version="1.0" encoding="UTF-8"?>
<beans  xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
                
                <bean id="emp" class="com.dhatri.Employee">
                <property name="employeeId"><value>DS-1339</value></property>
                <property name="employeeName"><value>Venkatesh</value></property>
                <property name="employeeSalary"><value>25000</value></property>
                </bean>
                <bean id="pointref" class="com.venkatesh.Point">
                <property name="x" value="8" />
                <property name="y" value="10" />
                 </bean>
				<bean id="triangleref" class="com.venkatesh.Triangle">
				<property name="point" ref="pointref"/>
				
				</bean>
</beans>
